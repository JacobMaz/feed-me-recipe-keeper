tate({ingredientIsOpen: true});\n                    this.setState({activeRecipe: recipe});\n                  }}\n                >\n                  Add Ingredient\n                </MenuItem>\n                <MenuItem onClick={() => {\n                  this.setState({drawer: true});\n                  this.setState({activeRecipe: recipe});\n                  }}> \n                  Ingredient Checklist\n                </MenuItem>\n                <MenuItem\n                  onClick={() => {\n                    this.setState({editOpen: true});\n                    this.setState({activeRecipe: recipe});\n                  }}\n                >\n                  Edit Recipe\n                </MenuItem>\n                <MenuItem onClick={() => this.setState({open: true})}>\n                  Delete Recipe\n                </MenuItem>\n              </Menu>\n            </React.Fragment>\n          )}\n        </PopupState>\n      </IconButton>\n    }\n    title={recipe.recipeName}\n    subheader={recipe.cuisine}\n  />\n                <CardMedia className={classes.media} image={recipeImage} />\n                <CardContent>\n                  <Typography\n                    variant=\"body2\"\n                    color=\"textSecondary\"\n                    component=\"p\"\n                  >\n                    Prep Time: {recipe.prepTime} mins\n                  </Typography>\n                  <Typography\n                    variant=\"body2\"\n                    color=\"textSecondary\"\n                    component=\"p\"\n                  >\n                    Cook Time: {recipe.cookTime} mins\n                  </Typography>\n                </CardContent>\n                <CardActions disableSpacing>\n                  <IconButton\n                    className={clsx(classes.expand, {\n                      [classes.expandOpen]: this.state.expanded,\n                    })}\n                    onClick={() => this.setState({expanded: !this.state.e